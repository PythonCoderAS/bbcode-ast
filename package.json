  "version": "1.1.0",
  "description": "Generate an AST of a BBCode fragment.",
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --require ts-node/register tests/**/*.test.ts",
